{
  "name": "simdive",
  "private": false,
  "version": "1.0.0",
  "description": "Simulateur d'ordinateur de plongée pédagogique pour l'enseignement de la décompression",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "build:analyze": "vite build --mode analyze",
    "preview": "vite preview",
    "preview:build": "vite preview --outDir dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "dependencies": {
    "chart.js": "^4.5.1",
    "vue": "^3.5.27",
    "vue-chartjs": "^5.3.3"
  },
  "devDependencies": {
    "@types/node": "^25.3.0",
    "@vitejs/plugin-vue": "^6.0.4",
    "@vue/test-utils": "^2.4.6",
    "vite-plugin-vue-devtools": "^8.0.6",
    "@vue/tsconfig": "^0.8.1",
    "vitest": "^4.0.18",
    "jsdom": "^27.4.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-vue": "^10.8.0",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0",
    "vite": "^7.2.4",
    "vue-tsc": "^3.2.4"
  },
  "keywords": [
    "diving",
    "scuba",
    "decompression",
    "education",
    "simulation",
    "vue"
  ],
  "author": "Sebastien <sebastien@abzu-diving.fr>",
  "license": "MIT",
  "homepage": "https://github.com/kisscool-fr/simdive",
  "repository": {
    "type": "git",
    "url": "https://github.com/kisscool-fr/simdive.git"
  },
  "bugs": {
    "url": "https://github.com/kisscool-fr/simdive/issues"
  },
  "engines": {
    "node": ">=24.0.0 <25.0.0",
    "npm": ">=11.6.0 <12.0.0"
  }
}
